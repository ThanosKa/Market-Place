{
  "name": "frontend",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "clear": "npx expo start --clear",
    "eject": "expo eject",
    "docker:build": "docker build -t marketplace-frontend .",
    "docker:run": "docker run -it -p 19000:19000 -p 19001:19001 -p 19002:19002 -e EXPO_DEVTOOLS_LISTEN_ADDRESS=0.0.0.0 -e REACT_NATIVE_PACKAGER_HOSTNAME=0.0.0.0 -e PROTOCOL=${PROTOCOL:-http} -e SERVER=${SERVER:-host.docker.internal} -e PORT=${PORT:-5001} -e API_BASEPATH=${API_BASEPATH:-api} --name marketplace-frontend-container marketplace-frontend",
    "docker:stop": "docker stop marketplace-frontend-container",
    "docker:remove": "docker rm marketplace-frontend-container",
    "docker:logs": "docker logs marketplace-frontend-container",
    "docker:shell": "docker exec -it marketplace-frontend-container /bin/bash"
  },
